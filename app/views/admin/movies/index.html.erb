
<h1>映画一覧</h1>
  <table>
    <tr>
      <th>ID</th>
      <th>タイトル</th>
      <th>公開年</th>
      <th>概要</th>
      <th>画像URL</th>
      <th>上映中</th>
      <th>登録日時</th>
      <th>更新日時</th>
    </tr>
    <% @movies.each do |t| %> 
    <tr>
      <%# <td><%= t.id %></td> %>
      <td><%= t.name %></td>
      <td><%= t.year %></td>
      <td><%= t.description %></td>
      <td><%= t.image_url %></td>
      <%# <td><%= t.is_showing %></td> %>
      <%# <td><%= t.created_at %></td> %>
      <%# <td><%= t.update_at %></td> %>
    </tr>
    <% end %>
  </table>
